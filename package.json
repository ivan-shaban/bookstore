{
  "name": "bookstore",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prisma:init": "npx prisma db push && npx prisma db seed",
    "prisma:format": "npx prisma format",
    "postprisma:format": "yarn prisma:generate-client",
    "prisma:generate-client": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "postinstall": "yarn prisma:generate-client",
    "predev": "yarn prisma:generate-client",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch --enableStatic",
    "build:path": "pathpida --ignorePath .gitignore --enableStatic",
    "prebuild": "yarn build:path",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "export": "next export",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.3",
    "@popperjs/core": "^2.11.5",
    "@prisma/client": "^3.14.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "classnames": "^2.3.1",
    "effector": "^22.2.0",
    "effector-react": "^22.0.6",
    "effector-storage": "^5.0.0",
    "formik": "^2.2.9",
    "http-status-codes": "^2.2.0",
    "moment": "^2.29.1",
    "next": "12.1.6",
    "next-auth": "^4.8.0",
    "next-sitemap": "^2.5.20",
    "pathpida": "^0.18.1",
    "react": "18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "18.1.0",
    "sass": "^1.49.9",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bootstrap": "^5.1.12",
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "cross-env": "^7.0.3",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-effector": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prettier-eslint-cli": "^6.0.1",
    "prisma": "^3.14.0",
    "ts-node": "^10.7.0",
    "ts-paths-esm-loader": "^1.3.0",
    "typescript": "4.6.4"
  }
}
